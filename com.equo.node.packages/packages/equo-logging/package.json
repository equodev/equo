{
    "name": "@equo/logging",
    "version": "0.1.4",
    "description": "Logging API for usage within the Equo Framework",
    "license": "UNLICENSED",
    "publishConfig": {
        "access": "restricted"
    },
    "main": "./lib/index.js",
    "typings": "./lib/index",
    "files": [
        "/lib"
    ],
    "dependencies": {
        "@equo/comm": "^0.1.5"
    },
    "devDependencies": {
