-include: ${build}/framework.bnd

-plugin.1.Chromium: \
    aQute.bnd.repository.p2.provider.P2Repository; \
        name = Chromium; \
        url = "https://dl.equoplatform.com/chromium-swt-ee/equo/p2"; \
        location = ~/.bnd/cache/chromium

-plugin.2.Release: \
    aQute.bnd.repository.p2.provider.P2Repository; \
        name = "Release"; \
        url = "https://dl.equoplatform.com/framework/${previous_version}/repo/"; \
        poll.time = -1

-baseline: *
-baselinerepo: Release