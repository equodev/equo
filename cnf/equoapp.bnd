-include: ext/equocore.bnd

-buildpath.equo: \
    com.equo.application.client;version=latest,\
    io.netty.all;version=latest,\
    com.equo.server.offline.api;version=latest,\
    com.equo.comm.api;version=latest,\
    org.eclipse.jface,\
    org.eclipse.swt

